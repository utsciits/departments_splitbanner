@media only screen and (min-width: 600px) {
  /* TOP MENU */
  #menu-bar {
    background: #fff;
    border-bottom: 1px solid #002554;
    height: 40px;
    padding-left: 0;
  }
  #menu-bar ul li {
    float: left;
  }
  #menu-bar a {
    color: #002554;
    display: block;
    font-weight: bold;
    line-height: 20px;
    padding: 12px 12px 8px 12px;
  }
  #menu-bar a.active,
  #menu-bar .active-trail {
    background: #bfb800;
  }
  #menu-bar #menu-281-1 a {
    background: url(images/home_btn_20x50.png) no-repeat 10px 10px;
    text-indent: -99999px;
    width: 19px;
  }
  #menu-bar #menu-281-1 a:hover {
    background: url(images/home_btn_20x50.png) no-repeat 10px 10px;
    text-indent: -99999px;
    width: 19px;
  }
  #menu-bar #menu-281-1 a.active {
    background: url(images/home_btn_20x50.png) no-repeat 10px 10px;
    text-indent: -99999px;
    width: 19px;
  }
  #menu-bar ul li ul li {
    border-bottom: 1px solid #e6e4e2;
  }
}
@media only screen and (min-width: 768px) {
  #menu-bar ul li ul {
    margin: 0;
    padding: 0;
    background: #ddd8a9;
    display: block;
    padding: 0 !important;
  }
  #menu-bar ul li ul li {
    background: #e6e4e2;
    border-bottom: 1px solid #e6e4e2;
    padding: 0;
  }
  #menu-bar ul li ul li a {
    background: #ddd8a9;
    border: none;
    color: #000;
    font-weight: normal;
    text-transform: none;
    width: auto;
    margin: 0;
  }
  #menu-bar ul li ul li a:hover,
  #menu-bar ul li:hover {
    background: #bfb800;
  }
}
