.boldUppercase {
  font-weight: bold;
  text-decoration: none;
  text-transform: uppercase;
}
.mainMenu {
  color: #002554;
  display: block;
  font-weight: bold;
}
.subNavBlock {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  -webkit-box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);
  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);
}
.marginPaddingZero {
  margin: 0;
  padding: 0;
}
html {
  font-size: 62.5%;
}
body {
  font-size: 15px;
  font-size: 1.5rem;
  background-color: #f4f4f4;
  color: #333333;
  font-family: sans-serif;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  color: #002554;
  font-family: "Trebuchet MS", sans-serif;
  font-weight: normal;
}
a {
  color: #4d6d92;
  text-decoration: none;
}
a:hover,
a:focus {
  color: #002554;
  text-decoration: underline;
}
a:visited {
  color: #334860;
}
#main-content p {
  line-height: 1.5em;
}
#main-content > header {
  margin: 0;
}
.region-sidebar-first {
  margin-left: 0 !important;
}
.sidebar-first .content-inner,
.two-sidebars .content-inner {
  margin: 0 !important;
}
.sidebar-first #main-content {
  float: left;
  width: 750px;
  margin-left: 2%;
}
.one-sidebar #main-content {
  margin: 2% 2% 0 0;
}
.sidebar-first #main-content {
  margin-right: 0;
  margin-left: 2%;
}
.two-sidebars #main-content {
  margin: 20px 2% 0 2%;
  width: 500px;
  float: left;
}
#page-title {
  font-size: 21px;
  font-size: 2.1rem;
  margin-bottom: .5em;
  padding-bottom: .5em;
  border-bottom: 1px solid #d7d4d2;
}
.view-employment-opportunities table {
  width: 100%;
}
.view-employment-opportunities table td,
.view-employment-opportunities th {
  padding: 10px;
}
.view-employment-opportunities th {
  background: #cccccc;
}
.view-employment-opportunities .views-field-title th {
  border: 1px solid #ffffff;
}
.teaser {
  margin-bottom: 10px;
  margin-top: 10px;
}
.teaser .article-content {
  margin: 10px;
}
.teaser .field-type-image {
  float: left;
  margin-right: 10px;
}
.teaser .article-title {
  padding: 10px 0 10px 0;
}
.teaser h3.article-title {
  margin-top: 10px;
}
.teaser p:first-of-type {
  margin-top: 0;
}
@media only screen and (min-width: 768px) {
  .view-cta-homepage .views-row {
    font-size: 14px;
    font-size: 1.4rem;
    float: left;
    margin-right: 3.38%;
    width: 230px;
    height: 280px;
    padding-bottom: 10px;
  }
  .view-cta-homepage h3 {
    font-size: 18px;
    font-size: 1.8rem;
    font-weight: bold;
    margin: 2px 0 5px 0;
    padding: 0;
  }
  .view-cta-homepage h4 a {
    color: #002554;
  }
  .view-cta-homepage h4 a:hover {
    color: #4d6d92;
  }
  .view-cta-homepage .views-row-1 {
    background: #6cace4;
  }
  .view-cta-homepage .views-row-2 {
    background: #a192b2;
  }
  .view-cta-homepage .views-row-3 {
    background: #7a9a01;
  }
  .view-cta-homepage .views-row-4 {
    background: #d38235;
  }
  .view-cta-homepage .views-row-5 {
    background: #c2b63a;
  }
  .view-cta-homepage .views-row-6 {
    background: #cbcabc;
  }
  .view-cta-homepage .views-row {
    margin-bottom: 20px;
  }
  .view-cta-homepage .views-row-3,
  #block-views-cta-home-block .views-row-6 {
    margin: 0;
  }
  .cta-coloured {
    padding: 0 10px;
  }
  .cta-coloured p {
    margin: 0;
  }
  #block-views-news-block,
  #block-views-events-block {
    float: left;
    margin-right: 3.38%;
    width: 230px;
  }
  #block-views-news-block .block-title,
  #block-views-events-block .block-title {
    font-weight: bold;
    font-size: 18px;
    font-size: 1.8rem;
    margin-bottom: 5px;
    background: #4d6d92;
    padding: 8px;
    color: #ffffff;
  }
  .view-news .views-row,
  .view-id-events .views-row {
    border-bottom: 1px solid #cccccc;
    padding: 20px 0 20px 0;
    height: 40px;
    border-bottom: 1px dashed #666666;
  }
  .view-id-faculty .views-row {
    border-bottom: 2px solid #cccccc;
    float: left;
    margin: 0 20px 20px 0;
    padding-bottom: 20px;
    width: 362px;
  }
  .view-id-faculty .views-row-even {
    margin: 0;
  }
  .view-id-faculty h2 {
    color: #333333;
    font-weight: bold;
  }
  .view-id-faculty img {
    float: left;
    margin-right: 20px;
    width: 104px;
  }
  ul.info {
    margin: 0;
    padding: 0;
    font-size: 14px;
    font-size: 1.4rem;
    list-style-type: none;
    float: left;
    width: 205px;
  }
  ul.info li {
    line-height: 20px;
  }
  li.full-bio {
    height: 40px;
    margin-top: 20px;
  }
  li.full-bio a {
    font-weight: bold;
    color: #333333;
    background: #cccccc;
    padding: 10px;
    margin-top: 30px;
    text-transform: uppercase;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    text-shadow: 0 1px 0 #ffffff;
  }
  .field-name-field-page-list {
    border-top: 1px dashed #cccccc;
  }
  .field-name-field-page-list p {
    font-size: 13px;
    font-size: 1.3rem;
  }
  .view-staff-contact .views-row {
    float: left;
    margin-right: 3.38%;
    width: 340px;
    height: 200px;
    border-bottom: 1px solid #cccccc;
    margin-bottom: 20px;
  }
  .region-sidebar-second {
    /*margin-top: 47px;*/
  }
  .view-id-news span.date-display-single {
    background: #d38235;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    font-size: 16px;
    font-size: 1.6rem;
    color: #ffffff;
    display: block;
    font-weight: bold;
    float: left;
    height: 40px;
    margin-right: 8px;
    padding: 5px;
    text-align: center;
    text-shadow: 1px 1px 1px #333, 0 0 0 #efefef, 1px 1px 1px #333;
    text-transform: uppercase;
    width: 40px;
  }
}
/*Variables*/
.listStyleNone {
  list-style-image: none;
  list-style-type: none;
}
/* SUPERFISH OVERWRITE*/
#menu-bar ul,
#menu-bar ul li {
  list-style-type: none;
  margin: 0;
  padding: 0;
}
#menu-bar ul li a {
  color: #002554;
  font-weight: bold;
  text-decoration: none;
  text-transform: uppercase;
}
#menu-bar a:hover {
  background: #ddd8a9;
}
#menu-bar .sf-vertical,
.sf-vertical li {
  width: auto;
}
#menu-bar .sf-menu.sf-style-simple.sf-vertical li,
.sf-sub-indicator,
.sf-menu.sf-style-default li .sf-menu.sf-style-default li li,
.sf-menu.sf-style-default.sf-navbar,
.sf-menu.sf-style-default a,
.sf-menu.sf-style-default li:hover,
.sf-menu.sf-style-default li.sfHover,
.sf-menu.sf-style-default a:focus,
.sf-menu.sf-style-default a:hover,
.sf-menu.sf-style-default a:active {
  background: #bfb800;
}
.sf-sub-indicator,
.sf-menu.sf-style-default li {
  background: none;
}
.sf-menu.sf-style-default a {
  background: none;
  border: none;
}
/* Left Navigation*/
.block-inner {
  margin: 20px 0;
}
.menu-block-1 {
  background: #d7d4d2;
  padding-bottom: 20px;
}
.menu-block-1 ul {
  margin: 0;
  padding: 0;
}
#block-menu-block-1 {
  margin-bottom: 20px;
}
#block-menu-block-1 li {
  margin: 0;
  padding: 0;
  border-bottom: 1px solid #f4f4f4;
  color: #002554;
  font-size: 14px;
  font-size: 1.4rem;
  font-weight: bold;
  list-style-position: inside;
}
#block-menu-block-1 li a {
  background: #e1dfdd;
  color: #333333;
  display: block;
  text-decoration: none;
  padding: 12px 20px;
}
#block-menu-block-1 ul li a:hover {
  background: #e1dfdd;
  cursor: pointer;
  font-weight: bold;
}
#block-menu-block-1 .active-trail {
  color: #002554;
}
ul li.leaf,
ul li.expanded,
ul li.collapsed {
  list-style-image: none;
  list-style-type: none;
}
#block-menu-block-1 ul li ul li a {
  background: #ddd8a9;
}
#block-menu-block-1 ul li ul li a:hover,
#block-menu-block-1 li.leaf .active-trail {
  background: #bfb800;
}
#block-menu-block-1 .collapsed ul li {
  background: url('images/menu-collapsed.png');
}
#block-menu-block-1 .expanded ul li {
  background: url('images/menu-expanded.png');
}
#block-menu-block-1 .active-trail,
#block-menu-block-1 li.expanded a:hover {
  background: #bfb800 url('images/menu-expanded.png') 5px center no-repeat;
}
.menu-block-1 .expanded ul li a {
  background: #efebc6;
  color: #333333;
  font-weight: 100;
}
#block-menu-block-1 li.expanded ul li .active,
#block-menu-block-1 li.expanded ul li a.active:hover,
#block-menu-block-1 a.active-trail.active,
#block-menu-block-1 .active {
  background: #bfb800 url('images/bg_nav.png') right no-repeat;
  color: #333333;
  font-weight: bold;
  margin-right: -5px;
}
/*line added by Sebastian*/
#block-menu-block-1 .expanded.active-trail.active > a.active-trail.active {
  background: #bfb800 url('images/menu-expanded.png') 5px center no-repeat;
  margin-right: 0;
}
#block-menu-block-1 ul li.collapsed a {
  background: #e1dfdd url('images/menu-collapsed.png') 5px center no-repeat;
}
#block-menu-block-1 ul li.collapsed a:hover {
  background: #d7d4d2 url('images/menu-collapsed.png') 5px center no-repeat;
}
#block-menu-block-1 .expanded ul.menu li a:hover {
  background-image: none;
}
